html
  head
    meta(charset="utf8")
    style *{padding: 0; margin: 0;}
  body(style="margin: 16px;")
    div#mainFrame(style=`border: solid 8px ${meta.DIFFICULTY.COLOR}; width: fit-content;`)
      div#titleBox(style=`background: ${meta.DIFFICULTY.COLOR}; padding-bottom: 8px; padding-left: 16px;`)
        span#title(style=`color: #FFFFFF; font-size: 36pt;`)= meta.TITLE
      div#scoreBox(style=`padding: 16px;`)
        div#score(style=`height: 576px; display: flex; align-items: flex-end;`)
          - for (let i = 0; i < Math.ceil(height/2304); i++)
            if -(height / 4 - 576 * i - 584 + 8) !== 568
              div(style="margin: 0 4px; width: 100px; height: fit-content;")
                div(style="width: 100px; margin-left: auto; overflow: hidden; height: 576px;")
                  img(src=`svg/${id}.svg`, style=`vertical-align: bottom; width: 100px; position: relative; top: ${-(height / 4 - 576 * i - 584 + 8)}px;`)
        div#footer(style=`text-align: right; height: 48px;`)
          span(style="font-size: 16pt; margin-top: 21px; margin-right: 6px; float: left; font-family: sans-serif;") Designer: #{meta.DESIGNER}
          img(src="sus2image.png",style="margin-top: 14px; height: 28pt; float: right; ")
          span(style="font-size: 16pt; margin-top: 21px; margin-right: 6px; float: right; font-family: sans-serif;") Generated By